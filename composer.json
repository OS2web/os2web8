{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8",
            "exclude": ["drupal/anonymous_subscriptions"]
        },
        {
            "type": "vcs",
            "url": "https://github.com/bellcom/anonymous_subscriptions"
        },
	{
            "type": "vcs",
            "url": "https://github.com/bellcom/fds_base_theme"
        },
	{	
		"type": "vcs",
		"url": "https://github.com/bellcom/access_latest"	
	}
    ],
    "require": {
        "bellcom/fds_base_theme": "dev-drupal10",
        "composer/installers": "^2.0",
        "cweagans/composer-patches": "^1.7",
        "drupal/access_latest": "dev-main",
        "drupal/admin_audit_trail": "^1.0",
        "drupal/admin_toolbar": "^3.5",
        "drupal/anonymous_subscriptions": "dev-drupal10",
        "drupal/autologout": "^1.4",
        "drupal/better_exposed_filters": "^6.0.6",
        "drupal/checklistapi": "^2.1",
        "drupal/chosen": "^4.0",
        "drupal/ckeditor_entity_link": "^1.0@dev",
        "drupal/ckeditor_iframe": "^3.0@alpha",
        "drupal/ckeditor_resize": "^1.6",
        "drupal/classy": "^1.0",
        "drupal/color": "^1.0",
        "drupal/config_delete": "^2.0",
        "drupal/content_access": "^2.0",
        "drupal/content_moderation_notifications": "^3.6",
        "drupal/core-composer-scaffold": "10.2.7",
        "drupal/core-project-message": "10.2.7",
        "drupal/core-recommended": "10.2.7",
        "drupal/crop": "^2.4",
        "drupal/csv_importer": "^1.16",
        "drupal/drush_language": "^1.0@RC",
        "drupal/editor_advanced_link": "^2.2",
        "drupal/embed": "1.7",
        "drupal/entity_clone": "^2.1@beta",
        "drupal/entity_reference_revisions": "^1.12",
        "drupal/facets": "^2.0",
        "drupal/fakeobjects": "^1.2",
        "drupal/feeds": "^3.0@beta",
        "drupal/feeds_ex": "^1.0@beta",
        "drupal/feeds_tamper": "dev-2.x",
        "drupal/field_group": "^3.6",
        "drupal/file_management": "^2.1",
        "drupal/flood_control": "^2.3",
        "drupal/form_options_attributes": "^2.0",
        "drupal/gdpr": "^3.1",
        "drupal/gdpr_dump": "^3.0",
        "drupal/image_effects": "^3.6",
        "drupal/image_widget_crop": "^2.4",
        "drupal/inline_entity_form": "^3.0@RC",
        "drupal/install_profile_generator": "^4.0@alpha",
        "drupal/link_field_autocomplete_filter": "^2.0",
        "drupal/linkit": "^6.1",
        "drupal/linkit_media_library": "^1.0",
        "drupal/media_directories": "^2.1@beta",
        "drupal/memcache": "^2.6",
        "drupal/memcache_admin": "^2.5",
        "drupal/menu_breadcrumb": "^2.0@alpha",
        "drupal/metatag": "^2.0",
        "drupal/migrate_plus": "^6.0",
        "drupal/migrate_tools": "^6.0",
        "drupal/monolog": "dev-2.x",
        "drupal/node_export": "^1.1",
        "drupal/node_revision_delete": "2.0.x-dev@dev",
        "drupal/paragraphs": "^1.17",
        "drupal/pathauto": "^1.13",
        "drupal/pathologic": "^2.0@alpha",
        "drupal/queue_ui": "^3.1",
        "drupal/quick_node_clone": "^1.18",
        "drupal/quickedit": "^1.0",
        "drupal/save_edit": "^1.8",
        "drupal/scheduler_content_moderation_integration": "dev-2.x",
        "drupal/search_api": "^1.34",
        "drupal/search_api_attachments": "^9.0",
        "drupal/search_api_autocomplete": "^1.8",
        "drupal/search_api_solr": "^4.3",
        "drupal/search_api_spellcheck": "^4.0",
        "drupal/search_api_synonym": "^2.0",
        "drupal/seven": "^1.0",
        "drupal/simplesamlphp_auth": "^4.0",
        "drupal/smtp": "^1.4",
        "drupal/stage_file_proxy": "3.0.x-dev@dev",
        "drupal/tamper": "^1.0@alpha",
        "drupal/token": "^1.15",
        "drupal/typed_data": "^2.0",
        "drupal/user_delete_reassign": "1.x-dev@dev",
        "drupal/video": "^3.0",
        "drupal/views_bulk_operations": "^4.2",
        "drupal/views_local_tasks": "^1.0",
        "drupal/webform": "^6.2",
        "drupal/workbench": "^1.4",
        "drupal/workbench_access": "^2.0",
        "drupal/workbench_moderation": "^1.7",
        "drupal/xmlsitemap": "^1.5",
        "drush/drush": "^12.5",
        "os2core/os2core": "dev-drupal10",
        "os2web/os2web": "dev-drupal10",
        "os2web/os2web_borgerdk": "dev-drupal10",
        "os2web/os2web_citizen_proposals": "dev-master",
        "os2web/os2web_contact": "dev-drupal10",
        "os2web/os2web_embedded_code": "dev-drupal10",
        "os2web/os2web_events": "dev-drupal10",
        "os2web/os2web_hearings": "dev-drupal10",
        "os2web/os2web_hjemmeside": "dev-drupal10",
        "os2web/os2web_kle": "dev-drupal10",
        "os2web/os2web_logging": "dev-master",
        "os2web/os2web_meetings": "dev-drupal10",
        "os2web/os2web_meetings_acadre": "dev-master",
        "os2web/os2web_meetings_agman": "dev-master",
        "os2web/os2web_meetings_edoc": "dev-drupal10",
        "os2web/os2web_meetings_edoc_fujitsu": "dev-drupal10",
        "os2web/os2web_news": "dev-drupal10",
        "os2web/os2web_newsletter": "dev-master",
        "os2web/os2web_pagebuilder": "dev-drupal10",
        "os2web/os2web_person_information": "dev-drupal10",
        "os2web/os2web_postlister": "dev-master",
        "os2web/os2web_rest_api": "dev-master",
        "os2web/os2web_search": "dev-drupal10",
        "os2web/os2web_simplesaml": "dev-master",
        "os2web/os2web_simplesaml_checklist": "dev-master",
        "os2web/os2web_spotbox": "dev-drupal10"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "phpstan/extension-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "php-http/discovery": true,
            "simplesamlphp/composer-module-installer": true,
            "drupal/console-extend-plugin": true,
            "cweagans/composer-patches": true,
            "zaporylie/composer-drupal-optimizations": true
        },
        "sort-packages": true
    },
    "extra": {
	"patches": {
      		"drupal/core": {
			"fix views pager offset": "https://www.drupal.org/files/issues/2024-03-06/mr-4707-vs-10.2.x.patch"
		}
    	},
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            },
	      "file-mapping": {
            	"[web-root]/.htaccess": false,
            	"[web-root]/robots.txt": false
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/installing-drupal",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    }
}
